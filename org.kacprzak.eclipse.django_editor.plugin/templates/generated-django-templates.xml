<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<templates>

   <template
        autoinsert="true"
        context="org.kacprzak.eclipse.django_editor.templates.contextType.django_tag"
        deleted="false"
        description="Django variable"
        enabled="true"
        name="{{ variable }}"
        id="org.kacprzak.eclipse.django_editor.templates.tags.{{ variable }}"
   >{{ ${variable} }}</template>

   <template
        autoinsert="true"
        context="org.kacprzak.eclipse.django_editor.templates.contextType.django_tag"
        deleted="false"
        description="Auto-escaping inside the block"
        enabled="true"
        name="autoescape"
        id="org.kacprzak.eclipse.django_editor.templates.tags.autoescape"
   >{% autoescape ${onoff} %}{% endautoescape %}</template>

   <template
        autoinsert="true"
        context="org.kacprzak.eclipse.django_editor.templates.contextType.django_tag"
        deleted="false"
        description="Define a block that can be overridden by child templates"
        enabled="true"
        name="block"
        id="org.kacprzak.eclipse.django_editor.templates.tags.block"
   >{% block ${name} %}{% endblock %}</template>

   <template
        autoinsert="true"
        context="org.kacprzak.eclipse.django_editor.templates.contextType.django_tag"
        deleted="false"
        description="Ignore everything between {% comment %} and {% endcomment %}"
        enabled="true"
        name="comment"
        id="org.kacprzak.eclipse.django_editor.templates.tags.comment"
   >{% comment %}  ${cursor}{% endcomment %}</template>

   <template
        autoinsert="true"
        context="org.kacprzak.eclipse.django_editor.templates.contextType.django_tag"
        deleted="false"
        description="easy-to-use protection against Cross Site Request Forgeries"
        enabled="true"
        name="csrf_token"
        id="org.kacprzak.eclipse.django_editor.templates.tags.csrf_token"
   >{% csrf_token %}</template>

   <template
        autoinsert="true"
        context="org.kacprzak.eclipse.django_editor.templates.contextType.django_tag"
        deleted="false"
        description="Cycles among the given strings each time through the loop"
        enabled="true"
        name="cycle"
        id="org.kacprzak.eclipse.django_editor.templates.tags.cycle"
   >{% cycle ${row1} ${row2} %}</template>

   <template
        autoinsert="true"
        context="org.kacprzak.eclipse.django_editor.templates.contextType.django_tag"
        deleted="false"
        description="Output a whole load of debugging information, including the current context and imported modules"
        enabled="true"
        name="debug"
        id="org.kacprzak.eclipse.django_editor.templates.tags.debug"
   >{% debug %}</template>

   <template
        autoinsert="true"
        context="org.kacprzak.eclipse.django_editor.templates.contextType.django_tag"
        deleted="false"
        description="Signal that this template extends a parent template"
        enabled="true"
        name="extends"
        id="org.kacprzak.eclipse.django_editor.templates.tags.extends"
   >{% extends ${file_name} %}</template>

   <template
        autoinsert="true"
        context="org.kacprzak.eclipse.django_editor.templates.contextType.django_tag"
        deleted="false"
        description="Filter the contents of the variable through variable filters"
        enabled="true"
        name="filter"
        id="org.kacprzak.eclipse.django_editor.templates.tags.filter"
   >{% filter ${name} %}{% endfilter %}</template>

   <template
        autoinsert="true"
        context="org.kacprzak.eclipse.django_editor.templates.contextType.django_tag"
        deleted="false"
        description="Outputs the first variable passed that is not False, without escaping"
        enabled="true"
        name="firstof"
        id="org.kacprzak.eclipse.django_editor.templates.tags.firstof"
   >{% firstof ${var1} ${var2} ${var3} %}</template>

   <template
        autoinsert="true"
        context="org.kacprzak.eclipse.django_editor.templates.contextType.django_tag"
        deleted="false"
        description="Loop over each item in an array"
        enabled="true"
        name="for"
        id="org.kacprzak.eclipse.django_editor.templates.tags.for"
   >{% for ${el} in ${array} %}{% endfor %}</template>

   <template
        autoinsert="true"
        context="org.kacprzak.eclipse.django_editor.templates.contextType.django_tag"
        deleted="false"
        description="The for tag can take an optional {% empty %} clause that will be displayed if the given array is empty or could not be found"
        enabled="true"
        name="forempty"
        id="org.kacprzak.eclipse.django_editor.templates.tags.forempty"
   >{% for ${el} in ${array} %}{% empty %}{% endfor %}</template>

   <template
        autoinsert="true"
        context="org.kacprzak.eclipse.django_editor.templates.contextType.django_tag"
        deleted="false"
        description="Evaluates a variable, and if that variable is true the contents of the block are output"
        enabled="true"
        name="if"
        id="org.kacprzak.eclipse.django_editor.templates.tags.if"
   >{% if ${condition} %}{% endif %}</template>

   <template
        autoinsert="true"
        context="org.kacprzak.eclipse.django_editor.templates.contextType.django_tag"
        deleted="false"
        description="The if tag can take an optional {% else %} clause that will be displayed if the test fails"
        enabled="true"
        name="ifelse"
        id="org.kacprzak.eclipse.django_editor.templates.tags.ifelse"
   >{% if ${condition} %}{% else %}{% endif %}</template>

   <template
        autoinsert="true"
        context="org.kacprzak.eclipse.django_editor.templates.contextType.django_tag"
        deleted="false"
        description="Check if a value has changed from the last iteration of a loop"
        enabled="true"
        name="ifchanged"
        id="org.kacprzak.eclipse.django_editor.templates.tags.ifchanged"
   >{% ifchanged ${what} %}{% endifchanged %}</template>

   <template
        autoinsert="true"
        context="org.kacprzak.eclipse.django_editor.templates.contextType.django_tag"
        deleted="false"
        description="Check if a value has changed from the last iteration of a loop"
        enabled="true"
        name="ifchangedelse"
        id="org.kacprzak.eclipse.django_editor.templates.tags.ifchangedelse"
   >{% ifchanged ${what} %}{% else %}{% endifchanged %}</template>

   <template
        autoinsert="true"
        context="org.kacprzak.eclipse.django_editor.templates.contextType.django_tag"
        deleted="false"
        description="Output the contents of the block if the two arguments equal each other"
        enabled="true"
        name="ifequal"
        id="org.kacprzak.eclipse.django_editor.templates.tags.ifequal"
   >{% ifequal ${par1} ${par2} %}{% endifequal %}</template>

   <template
        autoinsert="true"
        context="org.kacprzak.eclipse.django_editor.templates.contextType.django_tag"
        deleted="false"
        description="Output the contents of the block if the two arguments are NOT equal"
        enabled="true"
        name="ifnotequal"
        id="org.kacprzak.eclipse.django_editor.templates.tags.ifnotequal"
   >{% ifnotequal ${par1} ${par2} %}{% endifnotequal %}</template>

   <template
        autoinsert="true"
        context="org.kacprzak.eclipse.django_editor.templates.contextType.django_tag"
        deleted="false"
        description="Loads a template and renders it with the current context"
        enabled="true"
        name="include"
        id="org.kacprzak.eclipse.django_editor.templates.tags.include"
   >{% include ${file_name} %}</template>

   <template
        autoinsert="true"
        context="org.kacprzak.eclipse.django_editor.templates.contextType.django_tag"
        deleted="false"
        description="Load a custom template tag set"
        enabled="true"
        name="load"
        id="org.kacprzak.eclipse.django_editor.templates.tags.load"
   >{% load ${template} %}</template>

   <template
        autoinsert="true"
        context="org.kacprzak.eclipse.django_editor.templates.contextType.django_tag"
        deleted="false"
        description="This library allows specifying translatable text in templates. To enable it, set USE_I18N to True, then load it with"
        enabled="true"
        name="load_i18n"
        id="org.kacprzak.eclipse.django_editor.templates.tags.load_i18n"
   >{% load i18n %}</template>

   <template
        autoinsert="true"
        context="org.kacprzak.eclipse.django_editor.templates.contextType.django_tag"
        deleted="false"
        description="This library provides control over the localization of values in templates. You only need to load the library using {% load l10n %}, but you'll often set USE_L10N to True so that localization is active by default."
        enabled="true"
        name="load_l10n"
        id="org.kacprzak.eclipse.django_editor.templates.tags.load_l10n"
   >{% load l10n %}</template>

   <template
        autoinsert="true"
        context="org.kacprzak.eclipse.django_editor.templates.contextType.django_tag"
        deleted="false"
        description="Displays random 'lorem ipsum' Latin text. To use, add 'django.contrib.webdesign' to INSTALLED_APPS, then use {% load webdesign %} in a template."
        enabled="true"
        name="lorem"
        id="org.kacprzak.eclipse.django_editor.templates.tags.lorem"
   >{% lorem ${count} ${method} ${random} %}</template>

   <template
        autoinsert="true"
        context="org.kacprzak.eclipse.django_editor.templates.contextType.django_tag"
        deleted="false"
        description="Display the date, formatted according to the given string"
        enabled="true"
        name="now"
        id="org.kacprzak.eclipse.django_editor.templates.tags.now"
   >{% now ${format} %}</template>

   <template
        autoinsert="true"
        context="org.kacprzak.eclipse.django_editor.templates.contextType.django_tag"
        deleted="false"
        description="Regroup a list of alike objects by a common attribute"
        enabled="true"
        name="regroup"
        id="org.kacprzak.eclipse.django_editor.templates.tags.regroup"
   >{% regroup ${x} by ${y} as ${list} %}</template>

   <template
        autoinsert="true"
        context="org.kacprzak.eclipse.django_editor.templates.contextType.django_tag"
        deleted="false"
        description="Removes whitespace between HTML tags"
        enabled="true"
        name="spaceless"
        id="org.kacprzak.eclipse.django_editor.templates.tags.spaceless"
   >{% spaceless %}  ${cursor}{% endspaceless %}</template>

   <template
        autoinsert="true"
        context="org.kacprzak.eclipse.django_editor.templates.contextType.django_tag"
        deleted="false"
        description="Output the contents of a given file into the page"
        enabled="true"
        name="ssi"
        id="org.kacprzak.eclipse.django_editor.templates.tags.ssi"
   >{% ssi ${file_name} %}</template>

   <template
        autoinsert="true"
        context="org.kacprzak.eclipse.django_editor.templates.contextType.django_tag"
        deleted="false"
        description="Output one of the syntax characters used to compose template tags"
        enabled="true"
        name="templatetag"
        id="org.kacprzak.eclipse.django_editor.templates.tags.templatetag"
   >{% templatetag ${arg} %}</template>

   <template
        autoinsert="true"
        context="org.kacprzak.eclipse.django_editor.templates.contextType.django_tag"
        deleted="false"
        description="Returns an absolute URL (i.e., a URL without the domain name) matching a given view function and optional parameters"
        enabled="true"
        name="url"
        id="org.kacprzak.eclipse.django_editor.templates.tags.url"
   >{% url ${args} %}</template>

   <template
        autoinsert="true"
        context="org.kacprzak.eclipse.django_editor.templates.contextType.django_tag"
        deleted="false"
        description="Stops the template engine from rendering the contents of this block tag"
        enabled="true"
        name="verbatim"
        id="org.kacprzak.eclipse.django_editor.templates.tags.verbatim"
   >{% verbatim ${args} %}{% endverbatim %}</template>

   <template
        autoinsert="true"
        context="org.kacprzak.eclipse.django_editor.templates.contextType.django_tag"
        deleted="false"
        description="For creating bar charts and such, this tag calculates the ratio of a given value to a maximum value, and then applies that ratio to a constant"
        enabled="true"
        name="widthratio"
        id="org.kacprzak.eclipse.django_editor.templates.tags.widthratio"
   >{% widthratio ${this_value} ${max_value} 100 %}</template>

   <template
        autoinsert="true"
        context="org.kacprzak.eclipse.django_editor.templates.contextType.django_tag"
        deleted="false"
        description="Caches a complex variable under a simpler name"
        enabled="true"
        name="with"
        id="org.kacprzak.eclipse.django_editor.templates.tags.with"
   >{% with ${x} as ${y} %}{% endwith %}</template>

</templates>
